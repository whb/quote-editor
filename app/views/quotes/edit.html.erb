<div class="mx-auto md:w-2/3 w-full">
  <%= link_to sanitize("&larr; Back to quotes"), quotes_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  <h1 class="font-bold text-4xl">Editing quote</h1>

  <%= turbo_frame_tag @quote do %>
    <%= render "form", quote: @quote %>
  <% end %>

</div>
